            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-unused-vars": "off"
        }
    },
    "devDependencies": {
        "laravel-vite-plugin": "^1.0.0",
        "sass": "^1.83.4",
        "sass-loader": "^16.0.4",
        "vite": "^5.0.0",
        "vue-loader": "^16.8.3"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "jquery": "^3.7.1",
        "jspdf": "^2.5.2",
        "jspdf-autotable": "^3.8.4",
        "laravel-mix": "^6.0.49",
        "select2": "^4.1.0-rc.0",
        "sweetalert2": "^11.15.10",
        "vue": "^3.5.13",
        "vue-axios": "^3.5.2",
        "vue-multiselect": "^3.2.0",
        "vue-router": "^4.5.0",
        "vue-toast-notification": "^3.1.3",
